<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nate.frescostudydemo" >

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".activity.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.ShowProgressBarImgActivity"></activity>
        <activity android:name=".activity.CropImgActivity"></activity>
        <activity android:name=".activity.CircleAndCornerActivity"></activity>
        <activity android:name=".activity.JPEGImgActivity"></activity>
        <activity android:name=".activity.GifActivity"></activity>
        <activity android:name=".activity.MultiAndMultiPlexImgActivity"></activity>
        <activity android:name=".activity.ControllerListenerActivity"></activity>
        <activity android:name=".activity.ReSizeAndRotateActivity"></activity>
        <activity android:name=".activity.ModifyPicActivity"></activity>
    </application>

</manifest>
